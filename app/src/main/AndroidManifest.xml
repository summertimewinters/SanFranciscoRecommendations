<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.sanfranciscorecommendations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".CoffeeActivity"
            android:parentActivityName="com.example.android.sanfranciscorecommendations.MainActivity"
            android:label="@string/title_coffee" />
        <activity
            android:name=".RestaurantsActivity"
            android:parentActivityName="com.example.android.sanfranciscorecommendations.MainActivity"
            android:label="@string/title_restaurants" />
        <activity
            android:name=".ActivitiesActivity"
            android:parentActivityName="com.example.android.sanfranciscorecommendations.MainActivity"
            android:label="@string/title_activities" />
        <activity
            android:name=".BarsActivity"
            android:parentActivityName="com.example.android.sanfranciscorecommendations.MainActivity"
            android:label="@string/title_bars" />

    </application>

</manifest>